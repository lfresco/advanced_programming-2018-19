    "def is_palindrome(string_var):\n",
    "    \n",
    "    ret = True\n",
    "    for o,r in zip(string_var,''.join(reversed(string_var))):\n",
    "        ret = ret and (o == r)\n",
    "    return ret\n",
    "\n"
   ]
